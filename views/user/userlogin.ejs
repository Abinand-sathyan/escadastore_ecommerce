<!doctype html>
<html lang="en">
  <head>
  	<title>Login 10</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

	<link href="https://fonts.googleapis.com/css?family=Lato:300,400,700&display=swap" rel="stylesheet">

	<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
	
	<link rel="stylesheet" href="/user/UserLoginAssets/css/style.css">

	</head>
	<body class="img js-fullheight" style="background-image: url(/user/UserLoginAssets/images/slide-03.jpg);">
	<section class="ftco-section">
		<div class="container">
			<div class="row justify-content-center">
				<!-- <div class="col-md-6 text-center mb-5">
					<h2 class="heading-section">Login #10</h2>
				</div> -->
			</div>
			<div class="row justify-content-center">
				<div class="col-md-6 col-lg-4">
					<div class="login-wrap p-0">
		      	<h3 class="mb-4 text-center">Have an account?</h3>
		      	<form action="gethome" method="post" class="signin-form">
		      		<!-- <div class="form-group">
		      			<input type="text" class="form-control" placeholder="Username" required>
		      		</div> -->
					  <div class="form-group mb-3">
				       <input type="text" class="form-control" placeholder="email" name="email" value="safvan@gmail.com">
						<!-- <span class="icon fa fa-paper-plane-o"></span> -->
					</div>
	            <div class="form-group">
	              <input id="password-field" type="password" class="form-control" placeholder="Password" name="password" value="safvan" required>
	              <span toggle="#password-field" class="fa fa-fw fa-eye field-icon toggle-password"></span>
	            </div>
				<% if(usRblockERR.length>0){ %> 
					<div class=" alert alert-danger" role="alert">
					  <p class="text-danger"><%=usRblockERR %> </p>
					</div>
					<% } else if (passERR.length>0) { %>
						<div class=" alert alert-danger" role="alert">
							<p class="text-danger"><%=passERR %> </p>
						  </div>
					  <% } %> 
	            <div class="form-group">
	            	<button type="submit" class="form-control btn btn-primary submit px-3">Log In</button>
	            </div>
	            <div class="form-group d-md-flex">
	            	<div class="w-50">
		            	<label class="checkbox-wrap checkbox-primary">Remember Me
									  <input type="checkbox" checked>
									  <span class="checkmark"></span>
									</label>
								</div>
								<div class="w-50 text-md-right">
									<a href="#" style="color: #fff">Forgot Password</a>
								</div>
	            </div>
	          </form>
	          <a href="usersignup"><p class="w-100 text-center">&mdash;<a href="usersignup">Sign up&mdash;</p></a>
			  
	          <div class="social d-flex text-center">
	          	<a href="#" class="px-2 py-2 mr-md-1 rounded"><span class="ion-logo-facebook mr-2"></span> Facebook</a>
	          	<a href="#" class="px-2 py-2 ml-md-1 rounded"><span class="ion-logo-twitter mr-2"></span> Twitter</a>
	          </div>
		      </div>
				</div>
			</div>
		</div>
	</section>

	<script src="/user/UserLoginAssets/js/jquery.min.js"></script>
  <script src="/user/UserLoginAssets/js/popper.js"></script>
  <script src="/user/UserLoginAssets/js/bootstrap.min.js"></script>
  <script src="/user/UserLoginAssets/js/main.js"></script>

	</body>
</html>
